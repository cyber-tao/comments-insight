  {
    "id": "tiktok",
    "siteName": "TikTok",
    "domain": "tiktok.com",
    "container": {
      "selector": "[class*=\"DivCommentListContainer\"]",
      "type": "css"
    },
    "item": {
      "selector": "[class*=\"DivCommentObjectWrapper\"]",
