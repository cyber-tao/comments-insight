          "name": "username",
          "rule": {
            "selector": "a p",
            "type": "css"
          }
        },
        {
          "name": "content",
          "rule": {
            "selector": "[data-e2e=\"comment-level-2\"]",
            "type": "css"
          }
        },
        {
          "name": "timestamp",
          "rule": {
            "selector": "[class*=\"DivCommentSubContentWrapper\"] span:first-child",
            "type": "css"
          }
        },
        {
          "name": "likes",
