  {
    "id": "x",
    "siteName": "X (Twitter)",
    "domain": "x.com",
    "container": {
      "selector": "[data-testid=\"primaryColumn\"] section [aria-label]",
      "type": "css"
    },
    "item": {
      "selector": "article[data-testid=\"tweet\"]",
      "type": "css"
    },
    "fields": [
      {
        "name": "username",
        "rule": {
          "selector": "[data-testid=\"User-Name\"]",
          "type": "css"
        }
      },
      {
        "name": "content",
        "rule": {
          "selector": "[data-testid=\"tweetText\"]",
